{
   "$defs": {
      "AuthIpvAuthorisationResponseReceivedAuditEventClass": {
         "additionalProperties": false,
         "description": "Auth received a repsonse at the end of the IPV journey. Orchestration triggers upon receiving response from IPV (once IPV completes the Identity Journey).",
         "properties": {
            "client_id": {
               "description": "OAuth client identifier for the relying party",
               "type": "string"
            },
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^AUTH_IPV_AUTHORISATION_RESPONSE_RECEIVED$",
               "type": "string"
            },
            "user": {
               "$ref": "#/$defs/AuthIpvAuthorisationResponseReceivedUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "AuthIpvAuthorisationResponseReceivedAuditEventClass",
         "type": "object"
      },
      "AuthIpvAuthorisationResponseReceivedUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "email": {
               "description": "The user account email address.",
               "type": "string"
            },
            "govuk_signin_journey_id": {
               "description": "Description for govuk_signin_journey_id",
               "type": "string"
            },
            "persistent_session_id": {
               "description": "The persistent session id.",
               "type": "string"
            },
            "phone": {
               "description": "The user account phone number.",
               "type": "string"
            },
            "session_id": {
               "description": "The user session id.",
               "type": "string"
            },
            "user_id": {
               "description": "The pairwise identifier from the component of the system that submitted the event.",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthIpvAuthorisationResponseReceivedUserClass",
         "type": "object"
      }
   },
   "$id": "https://vocab.account.gov.uk/linkml/auth-ipv-authorisation-response-received",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "additionalProperties": false,
   "properties": {},
   "required": [],
   "title": "auth-ipv-authorisation-response-received",
   "type": "object"
}

