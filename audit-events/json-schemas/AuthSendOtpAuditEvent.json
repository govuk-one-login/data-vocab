{
   "$defs": {
      "AuthSendOtpAuditEventClass": {
         "additionalProperties": false,
         "description": "OTP sent. Triggered by Auth upon sending OTP to Email or Phone number.This will be triggered as part of the journey where user wants to update Email or Phone number through GOV.UK accounts",
         "properties": {
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^AUTH_SEND_OTP$",
               "type": "string"
            },
            "extensions": {
               "$ref": "#/$defs/AuthSendOtpExtensionsClass",
               "description": "The extended audit data relating to the event."
            },
            "user": {
               "$ref": "#/$defs/AuthSendOtpUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "AuthSendOtpAuditEventClass",
         "type": "object"
      },
      "AuthSendOtpExtensionsClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "notification_type": {
               "description": "The notification type used to notify the user",
               "type": "string"
            },
            "test_user": {
               "description": "If the user is a test user",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthSendOtpExtensionsClass",
         "type": "object"
      },
      "AuthSendOtpUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "email": {
               "description": "The user account email address.",
               "type": "string"
            },
            "ip_address": {
               "description": "The user ip address.",
               "type": "string"
            },
            "persistent_session_id": {
               "description": "The persistent session id.",
               "type": "string"
            },
            "phone": {
               "description": "The user account phone number.",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthSendOtpUserClass",
         "type": "object"
      }
   },
   "$id": "https://vocab.account.gov.uk/linkml/auth-send-otp",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "additionalProperties": false,
   "properties": {},
   "required": [],
   "title": "auth-send-otp",
   "type": "object"
}

