{
   "$defs": {
      "AuthAuthCodeIssuedAuditEventClass": {
         "additionalProperties": false,
         "description": "Code sent to user for MFA / This event is also fired at the end of a successful journey  in the one login platform. Triggered Just before sending the Authorisation code to Relying party.  If it is Auth + IPV journey, it will be triggered once the IPV is complete",
         "properties": {
            "client_id": {
               "description": "OAuth client identifier for the relying party",
               "type": "string"
            },
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^AUTH_AUTH_CODE_ISSUED$",
               "type": "string"
            },
            "extensions": {
               "$ref": "#/$defs/AuthAuthCodeIssuedExtensionsClass",
               "description": "The extended audit data relating to the event."
            },
            "user": {
               "$ref": "#/$defs/AuthAuthCodeIssuedUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "AuthAuthCodeIssuedAuditEventClass",
         "type": "object"
      },
      "AuthAuthCodeIssuedExtensionsClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "internalSubjectId": {
               "description": "The DI Pairwise or internal subject id",
               "type": "string"
            },
            "isNewAccount": {
               "description": "If the account is a newly created account",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthAuthCodeIssuedExtensionsClass",
         "type": "object"
      },
      "AuthAuthCodeIssuedUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "email": {
               "description": "The user account email address.",
               "type": "string"
            },
            "govuk_signin_journey_id": {
               "description": "Description for govuk_signin_journey_id",
               "type": "string"
            },
            "ip_address": {
               "description": "The user ip address.",
               "type": "string"
            },
            "persistent_session_id": {
               "description": "The persistent session id.",
               "type": "string"
            },
            "session_id": {
               "description": "The user session id.",
               "type": "string"
            },
            "user_id": {
               "description": "The pairwise identifier from the component of the system that submitted the event.",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthAuthCodeIssuedUserClass",
         "type": "object"
      }
   },
   "$id": "https://vocab.account.gov.uk/linkml/auth-auth-code-issued",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "additionalProperties": false,
   "properties": {},
   "required": [],
   "title": "auth-auth-code-issued",
   "type": "object"
}

