            "deviceId": {
               "description": "Device fingerprint",
               "type": "string"
            },
            "documentType": {
               "description": "The document type that a user has chosen to perform their identity checks with",
               "type": "string"
            },
            "drivingPermit": {
               "description": "Driving Licence information",
               "type": "string"
            },
            "issuingCountry": {
               "description": "The issuing country of the document a user has choosen to perform their identity checks with",
               "type": "string"
            },
            "name": {
               "description": "Name information",
               "type": "string"
            },
            "passport": {
               "description": "Passport information",
               "type": "string"
            },
            "residencePermit": {
               "description": "Residence Permit information",
               "type": "string"
            },
            "user": {
               "description": "The data relating to the user.",
               "type": "string"
            }
         },
         "required": [],
         "title": "DcmawCriVcIssuedRestrictedClass",
         "type": "object"
      },
      "DcmawCriVcIssuedUserClass": {
