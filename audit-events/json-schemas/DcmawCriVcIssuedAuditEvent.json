{
   "$defs": {
      "DcmawCriVcIssuedAuditEventClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "client_id": {
               "description": "OAuth client identifier for the relying party",
               "type": "string"
            },
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^DCMAW_CRI_VC_ISSUED$",
               "type": "string"
            },
            "evidence": {
               "$ref": "#/$defs/DcmawCriVcIssuedExtensionsEvidenceClass",
               "description": "Any evidence"
            },
            "extensions": {
               "$ref": "#/$defs/DcmawCriVcIssuedExtensionsClass",
               "description": "The extended audit data relating to the event."
            },
            "platform": {
               "$ref": "#/$defs/DcmawCriVcIssuedPlatformClass",
               "description": "The data relating to the user."
            },
            "restricted": {
               "$ref": "#/$defs/DcmawCriVcIssuedRestrictedClass",
               "description": "The restricted audit data relating to the event."
            },
            "user": {
               "$ref": "#/$defs/DcmawCriVcIssuedUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "DcmawCriVcIssuedAuditEventClass",
         "type": "object"
      },
      "DcmawCriVcIssuedExtensionsClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "evidence": {
               "description": "Any evidence",
               "type": "string"
            },
            "evidence_value": {
               "description": "Description for evidence_value",
               "type": "string"
            },
            "post_office_details": {
               "description": "Post office a user has chosen to complete their in-person checks at",
               "type": "string"
            },
            "toy": {
               "description": "Value submitted as part of the Toy CRI",
               "type": "string"
            },
            "toyResponse": {
               "description": "Response from mock external Toy CRI API",
               "type": "string"
            }
         },
         "required": [],
         "title": "DcmawCriVcIssuedExtensionsClass",
         "type": "object"
      },
      "DcmawCriVcIssuedExtensionsEvidenceClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "checkDetails": {
               "description": "The check details of the check carried out by the CRI",
               "type": "string"
            },
            "ci": {
               "description": "CIs in an array",
               "type": "string"
            },
            "failedCheckDetails": {
               "description": "The failed check details of the check carried out by the CRI",
               "type": "string"
            },
            "strengthScore": {
               "description": "The strengthScore from the check done by the CRI",
               "type": "string"
            },
            "txn": {
               "description": "Third party transaction id",
               "type": "string"
            },
            "validityScore": {
               "description": "The validityScore from the check done by the CRI",
               "type": "string"
            }
         },
         "required": [],
         "title": "DcmawCriVcIssuedExtensionsEvidenceClass",
         "type": "object"
      },
      "DcmawCriVcIssuedPlatformClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "device_info": {
               "description": "Device information object from DCMAW with data populated from Mobile App 3rd Party Suppliers",
               "type": "string"
            }
         },
         "required": [],
         "title": "DcmawCriVcIssuedPlatformClass",
         "type": "object"
      },
      "DcmawCriVcIssuedRestrictedClass": {
         "additionalProperties": false,
         "description": "",
