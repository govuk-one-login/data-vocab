{
   "$defs": {
      "AuthUpdateEmailAuditEventClass": {
         "additionalProperties": false,
         "description": "Update email (account).  Auth Triggers when email address (i.e. GOV.UK One login username) is updated through GOV.UK Account",
         "properties": {
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^AUTH_UPDATE_EMAIL$",
               "type": "string"
            },
            "user": {
               "$ref": "#/$defs/AuthUpdateEmailUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "AuthUpdateEmailAuditEventClass",
         "type": "object"
      },
      "AuthUpdateEmailUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "email": {
               "description": "The user account email address.",
               "type": "string"
            },
            "ip_address": {
               "description": "The user ip address.",
               "type": "string"
            },
            "persistent_session_id": {
               "description": "The persistent session id.",
               "type": "string"
            },
            "phone": {
               "description": "The user account phone number.",
               "type": "string"
            },
            "session_id": {
               "description": "The user session id.",
               "type": "string"
            },
            "user_id": {
               "description": "The pairwise identifier from the component of the system that submitted the event.",
               "type": "string"
            }
         },
         "required": [],
         "title": "AuthUpdateEmailUserClass",
         "type": "object"
      }
   },
   "$id": "https://vocab.account.gov.uk/linkml/auth-update-email",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "additionalProperties": false,
   "properties": {},
   "required": [],
   "title": "auth-update-email",
   "type": "object"
}

