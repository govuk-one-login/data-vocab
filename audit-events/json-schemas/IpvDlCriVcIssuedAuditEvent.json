{
   "$defs": {
      "IpvDlCriVcIssuedAuditEventClass": {
         "additionalProperties": false,
         "description": "When the final VC is created in the issue credential lambda",
         "properties": {
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^IPV_DL_CRI_VC_ISSUED$",
               "type": "string"
            },
            "evidence": {
               "$ref": "#/$defs/IpvDlCriVcIssuedExtensionsEvidenceClass",
               "description": "Any evidence"
            },
            "extensions": {
               "$ref": "#/$defs/IpvDlCriVcIssuedExtensionsClass",
               "description": "The extended audit data relating to the event."
            },
            "restricted": {
               "$ref": "#/$defs/IpvDlCriVcIssuedRestrictedClass",
               "description": "The restricted audit data relating to the event."
            },
            "user": {
               "$ref": "#/$defs/IpvDlCriVcIssuedUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "IpvDlCriVcIssuedAuditEventClass",
         "type": "object"
      },
      "IpvDlCriVcIssuedExtensionsClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "evidence": {
               "description": "Any evidence",
               "type": "string"
            },
            "evidence_value": {
               "description": "Description for evidence_value",
               "type": "string"
            },
            "iss": {
               "description": "issuer URI",
               "type": "string"
            }
         },
         "required": [],
         "title": "IpvDlCriVcIssuedExtensionsClass",
         "type": "object"
      },
      "IpvDlCriVcIssuedExtensionsEvidenceClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "activityScore": {
               "description": "The activityScore from the check done by the CRI",
               "type": "string"
            },
            "checkDetails": {
               "description": "The check details of the check carried out by the CRI",
               "type": "string"
            },
            "ci": {
               "description": "CIs in an array",
               "type": "string"
            },
            "failedCheckDetails": {
               "description": "The failed check details of the check carried out by the CRI",
               "type": "string"
            },
            "strengthScore": {
               "description": "The strengthScore from the check done by the CRI",
               "type": "string"
            },
            "txn": {
               "description": "Third party transaction id",
               "type": "string"
            },
            "type": {
               "description": "Description for type",
               "type": "string"
            },
            "validityScore": {
               "description": "The validityScore from the check done by the CRI",
               "type": "string"
            }
         },
         "required": [],
         "title": "IpvDlCriVcIssuedExtensionsEvidenceClass",
         "type": "object"
      },
      "IpvDlCriVcIssuedRestrictedClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "address": {
               "description": "Address information",
               "type": "string"
            },
            "birthDate": {
               "description": "DoB information",
               "type": "string"
            },
            "documentType": {
               "description": "The document type that a user has chosen to perform their identity checks with",
               "type": "string"
            },
            "drivingPermit": {
               "description": "Driving Licence information",
               "type": "string"
            },
            "issuingCountry": {
               "description": "The issuing country of the document a user has choosen to perform their identity checks with",
               "type": "string"
            },
            "name": {
               "description": "Name information",
               "type": "string"
            },
            "user": {
               "description": "The data relating to the user.",
               "type": "string"
            }
         },
         "required": [],
         "title": "IpvDlCriVcIssuedRestrictedClass",
         "type": "object"
      },
      "IpvDlCriVcIssuedUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "govuk_signin_journey_id": {
               "description": "Description for govuk_signin_journey_id",
               "type": "string"
            },
            "ip_address": {
               "description": "The user ip address.",
               "type": "string"
            },
            "persistent_session_id": {
               "description": "The persistent session id.",
               "type": "string"
            },
            "session_id": {
               "description": "The user session id.",
               "type": "string"
            },
            "user_id": {
               "description": "The pairwise identifier from the component of the system that submitted the event.",
               "type": "string"
            }
         },
         "required": [],
         "title": "IpvDlCriVcIssuedUserClass",
         "type": "object"
      }
   },
