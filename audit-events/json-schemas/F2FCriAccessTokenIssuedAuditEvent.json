{
   "$defs": {
      "F2FCriAccessTokenIssuedAuditEventClass": {
         "additionalProperties": false,
         "description": "An access token has been issued to a requester (as part of an OAuth flow)",
         "properties": {
            "client_id": {
               "description": "OAuth client identifier for the relying party",
               "type": "string"
            },
            "component_id": {
               "description": "The system component that generated the event",
               "type": "string"
            },
            "event_name": {
               "description": "The Audit Event type that is being submitted",
               "pattern": "^F2F_CRI_ACCESS_TOKEN_ISSUED$",
               "type": "string"
            },
            "user": {
               "$ref": "#/$defs/F2FCriAccessTokenIssuedUserClass",
               "description": "The data relating to the user."
            }
         },
         "required": [
            "event_name"
         ],
         "title": "F2FCriAccessTokenIssuedAuditEventClass",
         "type": "object"
      },
      "F2FCriAccessTokenIssuedUserClass": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "govuk_signin_journey_id": {
               "description": "Description for govuk_signin_journey_id",
               "type": "string"
            },
            "ip_address": {
               "description": "The user ip address.",
               "type": "string"
            },
            "session_id": {
               "description": "The user session id.",
               "type": "string"
            },
            "transaction_id": {
               "description": "The transaction id from the transaction that generated the audit event.",
               "type": "string"
            },
            "user_id": {
               "description": "The pairwise identifier from the component of the system that submitted the event.",
               "type": "string"
            }
         },
         "required": [],
         "title": "F2FCriAccessTokenIssuedUserClass",
         "type": "object"
      }
   },
   "$id": "https://vocab.account.gov.uk/linkml/f2f-cri-access-token-issued",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "additionalProperties": false,
   "properties": {},
   "required": [],
   "title": "f2f-cri-access-token-issued",
   "type": "object"
}

