{
  "name": "root",
  "description": "Builds the vocab site based on the identity vocab. Also generates JSON Schemas and TypeScript types from DI Vocab JSON Schema.",
  "private": true,
  "version": "1.5.0",
  "devDependencies": {
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govuk-one-login/data-vocab.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/govuk-one-login/data-vocab/issues"
  },
  "homepage": "https://github.com/govuk-one-login/data-vocab#readme",
  "browser": {
    "assert": false
  },
  "dependencies": {
    "json-schema-to-typescript": "^13.1.1"
  },
  "scripts": {
    "copy-pkg-misc": "cp code-generators/typescript-types/src/package-template.json code-generators/typescript-types/dist/package.json && cp README.md code-generators/typescript-types/dist",
    "generate": "npm run generate-types && npm run copy-pkg-misc",
    "generate-types": "node $PWD/code-generators/typescript-types/src/index.mjs $PWD/v1/json-schemas $PWD/code-generators/typescript-types/dist"
  }
}
